<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
	<!ENTITY marking "http://docs.oasis-open.org/ns/cti/stix/marking#">
	<!ENTITY markingContext "http://docs.oasis-open.org/ns/cti/stix/marking-context#">
	<!ENTITY owl "http://www.w3.org/2002/07/owl#">
	<!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
	<!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
	<!ENTITY stix "http://docs.oasis-open.org/ns/cti/stix#">
	<!ENTITY stixCore "http://docs.oasis-open.org/ns/cti/stix/core#">
	<!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>
<rdf:RDF xml:base="http://docs.oasis-open.org/ns/cti/stix/marking-context"
	xmlns="http://docs.oasis-open.org/ns/cti/stix/marking-context#"
	xmlns:marking="http://docs.oasis-open.org/ns/cti/stix/marking#"
	xmlns:markingContext="http://docs.oasis-open.org/ns/cti/stix/marking-context#"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:stix="http://docs.oasis-open.org/ns/cti/stix#"
	xmlns:stixCore="http://docs.oasis-open.org/ns/cti/stix/core#"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology rdf:about="http://docs.oasis-open.org/ns/cti/stix/marking-context">
		<owl:imports rdf:resource="http://docs.oasis-open.org/ns/cti/stix/marking"/>
		<owl:versionInfo>2.1.0</owl:versionInfo>
	</owl:Ontology>
	
	<rdf:Description rdf:about="&stixCore;created">
		<rdfs:domain rdf:resource="&marking;MarkingDefinition"/>
	</rdf:Description>
	
	<rdf:Description rdf:about="&stixCore;created_by_ref">
		<rdfs:domain rdf:resource="&marking;MarkingDefinition"/>
	</rdf:Description>
	
	<rdf:Description rdf:about="&stixCore;external_references">
		<rdfs:domain rdf:resource="&marking;MarkingDefinition"/>
	</rdf:Description>
	
	<rdf:Description rdf:about="&stixCore;id">
		<rdfs:domain rdf:resource="&marking;MarkingDefinition"/>
	</rdf:Description>
	
	<rdf:Description rdf:about="&stixCore;lang">
		<rdfs:domain rdf:resource="&marking;GranularMarking"/>
	</rdf:Description>
	
	<rdf:Description rdf:about="&stixCore;name">
		<rdfs:domain rdf:resource="&marking;MarkingDefinition"/>
	</rdf:Description>
	
	<rdf:Description rdf:about="&stixCore;spec_version">
		<rdfs:domain rdf:resource="&marking;MarkingDefinition"/>
	</rdf:Description>
	
	<rdf:Description rdf:about="&stixCore;type">
		<rdfs:domain rdf:resource="&marking;MarkingDefinition"/>
	</rdf:Description>
	
	<rdf:Description rdf:about="&marking;GranularMarking">
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&stixCore;lang"/>
				<owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&marking;marking_ref"/>
				<owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&marking;selectors"/>
				<owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
	</rdf:Description>
	
	<rdf:Description rdf:about="&marking;MarkingDefinition">
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&stixCore;created"/>
				<owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&stixCore;id"/>
				<owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&stixCore;spec_version"/>
				<owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&stixCore;type"/>
				<owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&marking;definition"/>
				<owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&marking;definition_type"/>
				<owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&stixCore;type"/>
				<owl:hasValue>marking-definition</owl:hasValue>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&stixCore;created_by_ref"/>
				<owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&stixCore;name"/>
				<owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
	</rdf:Description>
	
	<rdf:Description rdf:about="&marking;StatementMarking">
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&marking;statement"/>
				<owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
	</rdf:Description>
	
	<rdf:Description rdf:about="&marking;TLPMarking">
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&marking;tlp"/>
				<owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
	</rdf:Description>
	
	<rdf:Description rdf:about="&marking;definition">
		<rdfs:domain rdf:resource="&marking;MarkingDefinition"/>
	</rdf:Description>
	
	<rdf:Description rdf:about="&marking;definition_type">
		<rdfs:domain rdf:resource="&marking;MarkingDefinition"/>
		<rdfs:range>
			<rdfs:Datatype>
				<owl:unionOf rdf:parseType="Collection">
					<rdf:Description rdf:about="http://docs.oasis-open.org/ns/cti/stix/vocabulary#MarkingDefinitionTypeOpenVocab">
					</rdf:Description>
					<rdf:Description rdf:about="http://docs.oasis-open.org/ns/cti/stix/vocabulary#MarkingDefinitionTypeVocab">
					</rdf:Description>
				</owl:unionOf>
			</rdfs:Datatype>
		</rdfs:range>
	</rdf:Description>
	
	<rdf:Description rdf:about="&marking;granular_markings">
		<rdfs:domain rdf:resource="&marking;MarkingDefinition"/>
		<rdfs:range rdf:resource="&marking;GranularMarking"/>
	</rdf:Description>
	
	<rdf:Description rdf:about="&marking;marking_ref">
		<rdfs:domain rdf:resource="&marking;GranularMarking"/>
	</rdf:Description>
	
	<rdf:Description rdf:about="&marking;object_marking_refs">
		<rdfs:domain rdf:resource="&marking;MarkingDefinition"/>
	</rdf:Description>
	
	<rdf:Description rdf:about="&marking;selectors">
		<rdfs:domain rdf:resource="&marking;GranularMarking"/>
	</rdf:Description>
	
	<rdf:Description rdf:about="&marking;statement">
		<rdfs:domain rdf:resource="&marking;StatementMarking"/>
	</rdf:Description>
	
	<rdf:Description rdf:about="&marking;tlp">
		<rdfs:domain rdf:resource="&marking;TLPMarking"/>
	</rdf:Description>

</rdf:RDF>