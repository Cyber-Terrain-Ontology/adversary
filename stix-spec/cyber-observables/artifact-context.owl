<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
	<!ENTITY artifact "http://docs.oasis-open.org/cti/ns/stix/artifact#">
	<!ENTITY artifactCtx "http://docs.oasis-open.org/cti/ns/stix/artifact-context#">
	<!ENTITY owl "http://www.w3.org/2002/07/owl#">
	<!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
	<!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
	<!ENTITY stix "http://docs.oasis-open.org/cti/ns/stix#">
	<!ENTITY stixCore "http://docs.oasis-open.org/cti/ns/stix/core#">
	<!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>
<rdf:RDF xml:base="http://docs.oasis-open.org/cti/ns/stix/artifact-context"
	xmlns:artifact="http://docs.oasis-open.org/cti/ns/stix/artifact#"
	xmlns:artifactCtx="http://docs.oasis-open.org/cti/ns/stix/artifact-context#"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:stix="http://docs.oasis-open.org/cti/ns/stix#"
	xmlns:stixCore="http://docs.oasis-open.org/cti/ns/stix/core#"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology rdf:about="http://docs.oasis-open.org/cti/ns/stix/artifact-context">
		<owl:imports rdf:resource="http://docs.oasis-open.org/cti/ns/stix/artifact"/>
		<owl:imports rdf:resource="http://docs.oasis-open.org/cti/ns/stix/stix-core-context"/>
		<owl:versionInfo>2.1.0</owl:versionInfo>
	</owl:Ontology>
	
	<rdf:Description rdf:about="&stix;Artifact">
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&stixCore;id"/>
				<owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&stixCore;type"/>
				<owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&stixCore;type"/>
				<owl:hasValue>artifact</owl:hasValue>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&artifact;decryption_key"/>
				<owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&artifact;encryption_algorithm"/>
				<owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&artifact;payload_bin"/>
				<owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&stixCore;defanged"/>
				<owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&stixCore;mime_type"/>
				<owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&stixCore;spec_version"/>
				<owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&stixCore;url"/>
				<owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
	</rdf:Description>
	
	<rdf:Description rdf:about="&artifact;decryption_key">
		<rdfs:domain rdf:resource="&stix;Artifact"/>
	</rdf:Description>
	
	<rdf:Description rdf:about="&artifact;encryption_algorithm">
		<rdfs:domain rdf:resource="&stix;Artifact"/>
	</rdf:Description>
	
	<rdf:Description rdf:about="&artifact;payload_bin">
		<rdfs:domain rdf:resource="&stix;Artifact"/>
	</rdf:Description>
	
	<rdf:Description rdf:about="&stixCore;hashes">
		<rdfs:domain rdf:resource="&stix;Artifact"/>
	</rdf:Description>
	
	<rdf:Description rdf:about="&stixCore;mime_type">
		<rdfs:domain rdf:resource="&stix;Artifact"/>
	</rdf:Description>
	
	<rdf:Description rdf:about="&stixCore;url">
		<rdfs:domain rdf:resource="&stix;Artifact"/>
	</rdf:Description>

</rdf:RDF>