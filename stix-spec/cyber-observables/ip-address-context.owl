<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
	<!ENTITY cti "http://docs.oasis-open.org/cti/ns#">
	<!ENTITY ipAddr "http://docs.oasis-open.org/cti/ns/stix/ip-address#">
	<!ENTITY ipAddrCtx "http://docs.oasis-open.org/cti/ns/stix/ip-address/context#">
	<!ENTITY owl "http://www.w3.org/2002/07/owl#">
	<!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
	<!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
	<!ENTITY stix "http://docs.oasis-open.org/cti/ns/stix#">
	<!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>
<rdf:RDF xml:base="http://docs.oasis-open.org/cti/ns/stix/ip-address/context"
	xmlns:cti="http://docs.oasis-open.org/cti/ns#"
	xmlns:ipAddr="http://docs.oasis-open.org/cti/ns/stix/ip-address#"
	xmlns:ipAddrCtx="http://docs.oasis-open.org/cti/ns/stix/ip-address/context#"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:stix="http://docs.oasis-open.org/cti/ns/stix#"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology rdf:about="http://docs.oasis-open.org/cti/ns/stix/ip-address/context">
		<owl:imports rdf:resource="http://docs.oasis-open.org/cti/ns/stix/core/context"/>
		<owl:imports rdf:resource="http://docs.oasis-open.org/cti/ns/stix/ip-address"/>
		<owl:versionInfo>2.1.0</owl:versionInfo>
	</owl:Ontology>
	
	<rdf:Description rdf:about="&stix;IPv4Address">
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cti;id"/>
				<owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cti;object_type"/>
				<owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&ipAddr;value"/>
				<owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cti;object_type"/>
				<owl:hasValue>ipv4-addr</owl:hasValue>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cti;spec_version"/>
				<owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&stix;defanged"/>
				<owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
	</rdf:Description>
	
	<rdf:Description rdf:about="&stix;IPv6Address">
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cti;id"/>
				<owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cti;object_type"/>
				<owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&ipAddr;value"/>
				<owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cti;object_type"/>
				<owl:hasValue>ipv6-addr</owl:hasValue>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cti;spec_version"/>
				<owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&stix;defanged"/>
				<owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
	</rdf:Description>
	
	<rdf:Description rdf:about="&ipAddr;belongs_to_refs">
		<rdfs:domain rdf:resource="&stix;IPv4Address"/>
		<rdfs:domain rdf:resource="&stix;IPv6Address"/>
	</rdf:Description>
	
	<rdf:Description rdf:about="&ipAddr;belongs_to_refs_id">
		<rdfs:domain rdf:resource="&stix;IPv4Address"/>
		<rdfs:domain rdf:resource="&stix;IPv6Address"/>
	</rdf:Description>
	
	<rdf:Description rdf:about="&ipAddr;resolves_to_refs">
		<rdfs:domain rdf:resource="&stix;IPv4Address"/>
		<rdfs:domain rdf:resource="&stix;IPv6Address"/>
	</rdf:Description>
	
	<rdf:Description rdf:about="&ipAddr;resolves_to_refs_id">
		<rdfs:domain rdf:resource="&stix;IPv4Address"/>
		<rdfs:domain rdf:resource="&stix;IPv6Address"/>
	</rdf:Description>
	
	<rdf:Description rdf:about="&ipAddr;value">
		<rdfs:domain rdf:resource="&stix;IPv4Address"/>
		<rdfs:domain rdf:resource="&stix;IPv6Address"/>
	</rdf:Description>

</rdf:RDF>