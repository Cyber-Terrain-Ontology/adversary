<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
	<!ENTITY attackPattern "http://docs.oasis-open.org/ns/cti/stix/attack-pattern#">
	<!ENTITY attackPatternCtx "http://docs.oasis-open.org/ns/cti/stix/attack-pattern/context#">
	<!ENTITY cti "http://docs.oasis-open.org/ns/cti#">
	<!ENTITY owl "http://www.w3.org/2002/07/owl#">
	<!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
	<!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
	<!ENTITY stix "http://docs.oasis-open.org/ns/cti/stix#">
	<!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>
<rdf:RDF xml:base="http://docs.oasis-open.org/ns/cti/stix/attack-pattern/context"
	xmlns:attackPattern="http://docs.oasis-open.org/ns/cti/stix/attack-pattern#"
	xmlns:attackPatternCtx="http://docs.oasis-open.org/ns/cti/stix/attack-pattern/context#"
	xmlns:cti="http://docs.oasis-open.org/ns/cti#"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:stix="http://docs.oasis-open.org/ns/cti/stix#"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology rdf:about="http://docs.oasis-open.org/ns/cti/stix/attack-pattern/context">
		<owl:imports rdf:resource="http://docs.oasis-open.org/ns/cti/stix/attack-pattern"/>
		<owl:imports rdf:resource="http://docs.oasis-open.org/ns/cti/stix/core/context"/>
		<owl:versionInfo>2.1.0</owl:versionInfo>
	</owl:Ontology>
	
	<rdf:Description rdf:about="&cti;aliases">
		<rdfs:domain rdf:resource="&stix;AttackPattern"/>
	</rdf:Description>
	
	<rdf:Description rdf:about="&cti;description">
		<rdfs:domain rdf:resource="&stix;AttackPattern"/>
	</rdf:Description>
	
	<rdf:Description rdf:about="&cti;name">
		<rdfs:domain rdf:resource="&stix;AttackPattern"/>
	</rdf:Description>
	
	<rdf:Description rdf:about="&stix;AttackPattern">
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cti;created"/>
				<owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cti;id"/>
				<owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cti;modified"/>
				<owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cti;name"/>
				<owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cti;object_type"/>
				<owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cti;spec_version"/>
				<owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cti;object_type"/>
				<owl:hasValue>attack-pattern</owl:hasValue>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cti;confidence"/>
				<owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cti;created_by_ref"/>
				<owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cti;created_by_ref_id"/>
				<owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cti;description"/>
				<owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cti;lang"/>
				<owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cti;revoked"/>
				<owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
	</rdf:Description>
	
	<rdf:Description rdf:about="&stix;kill_chain_name">
		<rdfs:domain rdf:resource="&stix;AttackPattern"/>
	</rdf:Description>

</rdf:RDF>