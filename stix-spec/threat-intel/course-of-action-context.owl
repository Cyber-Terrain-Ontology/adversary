<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
	<!ENTITY coa "http://docs.oasis-open.org/cti/ns/stix/CourseOfAction#">
	<!ENTITY coaCtx "http://docs.oasis-open.org/cti/ns/stix/course-of-action-context#">
	<!ENTITY owl "http://www.w3.org/2002/07/owl#">
	<!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
	<!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
	<!ENTITY stix "http://docs.oasis-open.org/cti/ns/stix#">
	<!ENTITY stixCore "http://docs.oasis-open.org/cti/ns/stix/core#">
	<!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>
<rdf:RDF xml:base="http://docs.oasis-open.org/cti/ns/stix/course-of-action-context"
	xmlns:coa="http://docs.oasis-open.org/cti/ns/stix/CourseOfAction#"
	xmlns:coaCtx="http://docs.oasis-open.org/cti/ns/stix/course-of-action-context#"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:stix="http://docs.oasis-open.org/cti/ns/stix#"
	xmlns:stixCore="http://docs.oasis-open.org/cti/ns/stix/core#"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology rdf:about="http://docs.oasis-open.org/cti/ns/stix/course-of-action-context">
		<owl:imports rdf:resource="http://docs.oasis-open.org/cti/ns/stix/CourseOfAction"/>
		<owl:imports rdf:resource="http://docs.oasis-open.org/cti/ns/stix/stix-core-context"/>
		<owl:versionInfo>2.1.0</owl:versionInfo>
	</owl:Ontology>
	
	<rdf:Description rdf:about="&stix;CourseOfAction">
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&stixCore;created"/>
				<owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&stixCore;id"/>
				<owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&stixCore;modified"/>
				<owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&stixCore;name"/>
				<owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&stixCore;spec_version"/>
				<owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&stixCore;type"/>
				<owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&stixCore;type"/>
				<owl:hasValue>course-of-action</owl:hasValue>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&stixCore;confidence"/>
				<owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&stixCore;created_by_ref"/>
				<owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&stixCore;description"/>
				<owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&stixCore;lang"/>
				<owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&stixCore;revoked"/>
				<owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
	</rdf:Description>
	
	<rdf:Description rdf:about="&coa;action">
		<rdfs:domain rdf:resource="&stix;CourseOfAction"/>
	</rdf:Description>
	
	<rdf:Description rdf:about="&stixCore;description">
		<rdfs:domain rdf:resource="&stix;CourseOfAction"/>
	</rdf:Description>
	
	<rdf:Description rdf:about="&stixCore;name">
		<rdfs:domain rdf:resource="&stix;CourseOfAction"/>
	</rdf:Description>

</rdf:RDF>