    <!-- http://docs.oasis-open.org/tac/ns/security-playbook#impact -->

    <owl:DatatypeProperty rdf:about="http://docs.oasis-open.org/tac/ns/security-playbook#impact">
        <rdfs:domain rdf:resource="http://docs.oasis-open.org/tac/ns/security-playbook#SecurityPlaybook"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</xsd:minInclusive>
                    </rdf:Description>
                    <rdf:Description>
                        <xsd:maxInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">100</xsd:maxInclusive>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
        <rdfs:comment xml:lang="en-us">An integer that represents the impact the playbook has on the organization from 0 to 100. A value of 0 means specifically undefined. Values range from 1, the lowest impact, to a value of 100, the highest. For example, a purely investigative playbook that is non-invasive would have a low impact value of 1, whereas a playbook that performs changes such as adding rules into
        a firewall would have a higher impact value.</rdfs:comment>
        <rdfs:label>impact</rdfs:label>
    </owl:DatatypeProperty>
    

    <!-- http://docs.oasis-open.org/tac/ns/security-playbook#organization_type -->

    <owl:DatatypeProperty rdf:about="http://docs.oasis-open.org/tac/ns/security-playbook#organization_type">
        <rdfs:domain rdf:resource="http://docs.oasis-open.org/tac/ns/security-playbook#SecurityPlaybook"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en-us">The type of organization that the playbook is intended for. This can be an industry sector.</rdfs:comment>
        <rdfs:label>organization_type</rdfs:label>
    </owl:DatatypeProperty>
    

    <!-- http://docs.oasis-open.org/tac/ns/security-playbook#playbook -->

    <owl:DatatypeProperty rdf:about="http://docs.oasis-open.org/tac/ns/security-playbook#playbook">
        <rdfs:domain rdf:resource="http://docs.oasis-open.org/tac/ns/security-playbook#SecurityPlaybook"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en-us">The whole playbook in its native format (e.g., CACAO JSON - Stringified). Security playbook producers and consumers of playbooks use this property to share and retrieve playbooks.</rdfs:comment>
        <rdfs:label>playbook</rdfs:label>
    </owl:DatatypeProperty>
    

    <!-- http://docs.oasis-open.org/tac/ns/security-playbook#playbook_abstraction -->

    <owl:DatatypeProperty rdf:about="http://docs.oasis-open.org/tac/ns/security-playbook#playbook_abstraction">
        <rdfs:domain rdf:resource="http://docs.oasis-open.org/tac/ns/security-playbook#SecurityPlaybook"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://docs.oasis-open.org/tac/ns/security-playbook#SecurityPlaybookAbstractionLevelOpenVocab"/>
                    <rdf:Description rdf:about="http://docs.oasis-open.org/tac/ns/security-playbook#SecurityPlaybookAbstractionLevelVocab"/>
                </owl:unionOf>
            </rdfs:Datatype>
        </rdfs:range>
        <rdfs:comment xml:lang="en-us">Identifies the playbook abstraction level.</rdfs:comment>
        <rdfs:label>playbook_abstraction</rdfs:label>
    </owl:DatatypeProperty>


    <!-- http://docs.oasis-open.org/tac/ns/security-playbook#playbook_base64 -->

    <owl:DatatypeProperty rdf:about="http://docs.oasis-open.org/tac/ns/security-playbook#playbook_base64">
        <rdfs:domain rdf:resource="http://docs.oasis-open.org/tac/ns/security-playbook#SecurityPlaybook"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en-us">The whole playbook encoded in base64. Security playbook producers and consumers of playbooks use this property to share and retrieve playbooks.</rdfs:comment>
    </owl:DatatypeProperty>
    

    <!-- http://docs.oasis-open.org/tac/ns/security-playbook#playbook_standard -->

    <owl:DatatypeProperty rdf:about="http://docs.oasis-open.org/tac/ns/security-playbook#playbook_standard">
        <rdfs:domain rdf:resource="http://docs.oasis-open.org/tac/ns/security-playbook#SecurityPlaybook"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en-us">Identification of the playbook standard (e.g., CACAO).</rdfs:comment>
        <rdfs:label>playbook_standard</rdfs:label>
    </owl:DatatypeProperty>
    

    <!-- http://docs.oasis-open.org/tac/ns/security-playbook#playbook_type -->

    <owl:DatatypeProperty rdf:about="http://docs.oasis-open.org/tac/ns/security-playbook#playbook_type">
        <rdfs:domain rdf:resource="http://docs.oasis-open.org/tac/ns/security-playbook#SecurityPlaybook"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://docs.oasis-open.org/tac/ns/security-playbook#SecurityPlaybookTypeOpenVocab"/>
                    <rdf:Description rdf:about="http://docs.oasis-open.org/tac/ns/security-playbook#SecurityPlaybookTypeVocab"/>
                </owl:unionOf>
            </rdfs:Datatype>
        </rdfs:range>
        <rdfs:comment xml:lang="en-us">The security-focused operational functions the playbook addresses. A playbook may account for multiple types (e.g., detection and investigation).</rdfs:comment>
        <rdfs:label>playbook_type</rdfs:label>
    </owl:DatatypeProperty>
    

    <!-- http://docs.oasis-open.org/tac/ns/security-playbook#priority -->

    <owl:DatatypeProperty rdf:about="http://docs.oasis-open.org/tac/ns/security-playbook#priority">
        <rdfs:domain rdf:resource="http://docs.oasis-open.org/tac/ns/security-playbook#SecurityPlaybook"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</xsd:minInclusive>
                    </rdf:Description>
                    <rdf:Description>
                        <xsd:maxInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">100</xsd:maxInclusive>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
        <rdfs:comment xml:lang="en-us">An integer that represents the priority of this playbook relative to other defined playbooks. A value of 0 means specifically undefined. Values range from 1, the highest priority, to a value of 100, the lowest.</rdfs:comment>
        <rdfs:label>priority</rdfs:label>
    </owl:DatatypeProperty>
    

    <!-- http://docs.oasis-open.org/tac/ns/security-playbook#severity -->

    <owl:DatatypeProperty rdf:about="http://docs.oasis-open.org/tac/ns/security-playbook#severity">
        <rdfs:domain rdf:resource="http://docs.oasis-open.org/tac/ns/security-playbook#SecurityPlaybook"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</xsd:minInclusive>
                    </rdf:Description>
                    <rdf:Description>
                        <xsd:maxInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">100</xsd:maxInclusive>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
        <rdfs:comment xml:lang="en-us">An integer that represents the seriousness of the conditions that this playbook addresses. A value of 0 means specifically undefined. Values range from 1, the lowest severity, to a value of 100, the highest.</rdfs:comment>
        <rdfs:label>severity</rdfs:label>
    </owl:DatatypeProperty>
    


